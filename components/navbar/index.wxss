.navbar {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  text-align: center;
  color: #333;
  font-size: 16px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  left: 0;
  z-index: 10;
}

.navbar-action-wrap {
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  z-index: 11;
  line-height: 1;
  padding: 4px 0 4px 10px;
}
.navbar-action-wrap .icon-back {
  font-size: 20px;
  font-weight: bold;
}
.navbar-action-group {
  border: 2rpx solid #f0f0f0;
  border-radius: 20px;
  overflow: hidden;
}

.navbar-action_item {
  padding: 3px 0;
  color: #333;
}

.navbar-action-group .navbar-action_item {
  border-right: 1px solid #f0f0f0;
  padding: 3px 14px;
}

.navbar-action-group .last {
  border-right: none;
}
