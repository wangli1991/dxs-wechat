.back-top {
  width: 90rpx;
  height: 90rpx;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 50%;
  overflow: hidden;
  position: fixed;
  z-index: 999;
  bottom: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.back-top .iconfont {
  color: #fff;
  font-size: 26rpx;
}
.back-top .text {
  color: #fff;
  font-size: 22rpx;
  line-height: 30rpx;
}
