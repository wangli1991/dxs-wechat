/* 公共样式 */
view {
  box-sizing: border-box;
}
page {
  height: calc(100% - env(safe-area-inset-bottom));
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  position: relative;
  z-index: 999;
}
.navbar {
  flex: 0;
  flex-basis: auto;
}
.container {
  flex: 1;
  overflow: hidden;
  background-color: #f4f5fa;
}
.ff-num {
  font-family: Arial;
}
